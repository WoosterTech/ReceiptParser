{
  "configurations": [
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "receiptparser\\app.py",
        "FLASK_DEBUG": "1",
        "SECRET_KEY": "test_secret_key"
      },
      "args": ["run", "--no-reload"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Python: GUI",
      "type": "python",
      "request": "launch",
      "program": "receiptparser\\extract.py",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
